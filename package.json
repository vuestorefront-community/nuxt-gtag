{
  "name": "@vue-storefront/nuxt-gtag",
  "version": "1.0.0",
  "license": "MIT",
  "contributors": [
    {
      "email": "heitor.ramon@gmail.com",
      "name": "Heitor Ramon Ribeiro",
      "url": "https://heitor.co"
    }
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint src --ext .js,.ts,.vue --ignore-path .eslintignore --fix",
    "lint:fix": "eslint src --ext .js,.ts,.vue --ignore-path .eslintignore  --fix",
    "precommit": "lint-staged",
    "prepack": "nuxt-module-build",
    "prepare": "node ./scripts/prepare.js",
    "postinstall": "npm run dev:prepare",
    "release": "npx git-ensure -a && npx bumpp --commit --tag --push"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.6",
    "transform-object-keys": "^1.0.7",
    "ufo": "^0.8.5"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/prettier": "^2.6.4",
    "@vue-storefront/eslint-config": "^1.0.17",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "latest",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.0.0-rc.6",
    "prettier": "^2.7.1"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ]
}
